{
  "name": "someline/starter-framework",
  "type": "framework",
  "description": "The Someline Starter Framework.",
  "keywords": [
    "someline",
    "starter-framework"
  ],
  "homepage": "https://github.com/someline/starter-framework",
  "license": "MIT",
  "authors": [
    {
      "name": "Libern Lin",
      "email": "libernlin@gmail.com",
      "homepage": "https://www.someline.com/",
      "role": "Developer"
    }
  ],
  "require": {
    "php": ">=5.5.9",
    "ext-pdo_sqlite": "*",
    "laravel/framework": "5.3.*",
    "predis/predis": "~1.0",
    "doctrine/dbal": "~2.3",
    "itsgoingd/clockwork": "~1.10",
    "dingo/api": "1.0.x@dev",
    "guzzlehttp/guzzle": "~6.0",
    "lukasoppermann/http-status": "^2.0",
    "prettus/l5-repository": "^2.1",
    "prettus/laravel-validation": "1.1.*",
    "intervention/image": "^2.3",
    "intervention/imagecache": "~2.1",
    "cviebrock/image-validator": "2.0.*@beta",
    "barryvdh/laravel-ide-helper": "^2.1",
    "someline/rest-api-client": "1.1.*",
    "someline/someline-image": "^1.0",
    "mcamara/laravel-localization": "1.1.*",
    "laravel/passport": "^1.0",
    "monarobase/country-list": "^1.2",
    "libern/timezone": "^1.0",
    "giggsey/libphonenumber-for-php": "~7.0",
    "torann/geoip": "0.2.*@dev"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0||~5.0",
    "scrutinizer/ocular": "~1.1",
    "squizlabs/php_codesniffer": "~2.3"
  },
  "autoload": {
    "files": [
      "src/Someline/Foundation/helpers.php"
    ],
    "psr-4": {
      "Someline\\": "src/Someline"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Someline\\Test\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit",
    "format": "phpcbf --standard=psr2 src/"
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "minimum-stability": "dev"
}
